<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xsl:stylesheet version="2.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">

	<!-- In this update we'll add the executor service server sensor to existing environment(s)-->

	<!--Copy all -->
	<xsl:template match="@*|node()">
		<xsl:copy>
			<xsl:apply-templates select="@*|node()" />
		</xsl:copy>
	</xsl:template>

	<!--Add sensor type to method-sensor-configs -->
	<xsl:template match="environment/method-sensor-configs">
		<xsl:copy>
			<xsl:apply-templates select="@* | node()"/>
			<xsl:element name="executor-client-sensor-config"/>
		</xsl:copy>
	</xsl:template>

</xsl:stylesheet>