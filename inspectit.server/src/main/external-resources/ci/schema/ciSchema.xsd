                  <xs:element ref="remote-spring-resttemplate-client-sensor-config"/>
                  <xs:element ref="remote-jms-client-sensor-config"/>
                  <xs:element ref="remote-manual-server-sensor-config"/>
                </xs:choice>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element ref="exception-sensor-config"/>
          <xs:element ref="jmx-loading-sensor-config"/>
          <xs:element name="profile-ids" minOccurs="0">
