              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="classLoadingDelegation" type="xs:boolean"/>
          <xs:element name="retransformation-strategy" type="retransformationStrategy" minOccurs="0"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
