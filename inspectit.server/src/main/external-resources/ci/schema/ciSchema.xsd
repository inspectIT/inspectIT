  <xs:complexType name="businessTransactionDefinition">
    <xs:sequence>
      <xs:element ref="name-extraction" minOccurs="0"/>
      <xs:choice>
        <xs:element ref="and"/>
        <xs:element ref="or"/>
        <xs:element ref="not"/>
        <xs:element ref="string-matching"/>
