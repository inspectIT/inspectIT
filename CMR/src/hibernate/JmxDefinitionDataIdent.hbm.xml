<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="info.novatec.inspectit.cmr.model">
	<class name="JmxDefinitionDataIdent" table="jmxDefinitionData_ident">
		<id name="id" type="java.lang.Long" column="id">
			<generator class="seqhilo" />
		</id>
		<property name="timeStamp">
			<column name="time_stamp" />
		</property>
		<many-to-one name="platformIdent" column="platform_id" class="PlatformIdent" />
		<property name="mBeanObjectName">
			<column name="mBeanObjectName" />
		</property>
		<property name="mBeanAttributeName">
			<column name="mBeanAttributeName" />
		</property>
		<property name="mBeanAttributeDescription">
			<column name="mBeanAttributeDescription" />
		</property>
		<property name="mBeanAttributeType">
			<column name="mBeanAttributeType" />
		</property>
		<property name="mBeanAttributeIsIs">
			<column name="mBeanAttributeIsIs" />
		</property>
		<property name="mBeanAttributeIsReadable">
			<column name="mBeanAttributeIsReadable" />
		</property>
		<property name="mBeanAttributeIsWritable">
			<column name="mBeanAttributeIsWritable" />
		</property>
	</class>
</hibernate-mapping>